{
  "manifest_version": 1,
  "type": "WebApp",
  "meta":{
    "name": "Heroes of the Storm Talent Picks",
    "author": "David Filipidisz",
    "version": "3.1.5",
    "minimum-overwolf-version": "0.82.0",
    "access-name": "com.davidf.hotstalents",
    "description": "Up-to-date Heroes of the Storm talent statistics for every Hero, ordered by popularity or win rate.",
    "dock_button_title": "HoTS Talent Stats",
    "icon": "IconMouseOver.png",
    "icon_gray": "IconMouseNormal.png"
  },
  "permissions": ["Profile"],
  "data": {
    "start_window": "MainWindow",
    "externally_connectable": {
      "matches": [
        "https://fonts.googleapis.com/*",
        "https://raw.githubusercontent.com/*",
        "http://owanalytics.noip.me/*"
      ]
    },
    "windows": {
      "MainWindow": {
        "file": "Files/index.html",
        "transparent": true,
        "resizable": true,
        "size": {"width":470, "height":367},
        "min_size": {"width":470, "height":367},
        "max_size": {"width":2000, "height":2000}
      }
    }
  }
}
